{
  "name": "webvr-boilerplate",
  "version": "0.4.1",
  "description": "A starting point for web-based VR experiences that work in both Cardboard and Oculus.",
  "main": "src/main.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "webvr-polyfill": "^0.9.0",
